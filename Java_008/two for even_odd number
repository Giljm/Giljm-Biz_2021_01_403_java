package com.callo.start;

public class Odd_Even_01 {

	public static void main(String[] args) {

		int num1 = 0;

		for (int sw = 1; sw <= 100; sw++) {

			if (sw % 2 == 0) { 
				num1 += sw; // 값을 누적한다.
			}
		}
		System.out.println(num1);

		int num2 = 0;

		for (int sw = 0; sw < 100; sw++) {

			if (sw % 2 == 1) {
				num2 += sw;
			}
		}
		System.out.println(num2);
	}

}
